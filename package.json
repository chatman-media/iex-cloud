    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "ts-node": "^8.5.4",
    "typedoc": "^0.15.4",
    "typedoc-neo-theme": "^1.0.4",
    "typescript": "^3.7.3"
  },
  "dependencies": {
    "dotenv": "^8.2.0",
    "node-fetch": "^2.6.0"
